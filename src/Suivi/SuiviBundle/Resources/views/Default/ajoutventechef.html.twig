<div id="masthead">
    <div class="container">
        <div class="row">
            <div class="col-md-7">
                <h1>SUIVI VENTE
                    <p class="lead"></p>
                </h1>
            </div>
            <div class="col-md-5">
                <div class="well well-lg">
                    <div class="row">
                        <div class="col-sm-12">

                            <img src="{{asset("bundles/adminTemplate/img/tt400.jpg")}}">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div><!-- /cont -->

